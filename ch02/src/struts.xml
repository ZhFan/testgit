<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<include file="struts-default.xml"/>
	<package name="org.zf.struts2.ch02.action" extends="undefined package struts-default">
  		<action name="ListAction" class="org.zf.struts2.ch02.action.BookAction" method="Search">
			<result name="success" >/BookList.jsp</result></action>
		<action name="DetailAction" class="org.zf.struts2.ch02.action.BookAction" method="Detail">
			<result name="success" >/Bookdetail.jsp</result></action>
		<action name="DeleteAction" class="org.zf.struts2.ch02.action.BookAction" method="Delete">
			<result name="success" >/BookList.jsp</result></action>
	</package>
</struts>
 